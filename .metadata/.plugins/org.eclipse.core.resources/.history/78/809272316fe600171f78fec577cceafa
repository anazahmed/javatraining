package com.digitek.business;

public class IfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  weekday(2);
	}
	public static void weekday(int days) {
		if (days==1) {
			System.out.println("monday");
		}
		else (days==2) {
			System.out.println("Tuesday");
		}
		
	
	}
		
}	
