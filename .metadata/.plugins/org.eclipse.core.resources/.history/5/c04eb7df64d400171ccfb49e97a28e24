package com.digitek.mathoperations;

public class SubtractionExamples {
	
	public static void subTwoNumbers() {
		 int a = 50;
		 int b = 30;
		 int result = a-b;
		 System.out.println(result);
	}
public static void subThreeNumbers() {
	 int a = 50;
	 int b = 30;
	 int c = 10;
	 int result = a-b-c;
	 System.out.println(result);
}
	public static void main(String[] args) {
		SubtractionExamples ae = newSubtractionExamples();
		ae.subTwoNumbers();
		ae.subThreeNumbers();
		

	}

}
